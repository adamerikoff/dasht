~qzq
болсын СӨЙЛЕМ = "Сәлем"
болсын ТУРЕЦКОЕ_СЛОВО = "merhaba"
болсын ТУРЕЦКАЯ_ФРАЗА_ДОБАВЛЕНА = шын
болсын БАЗА_ЧИСЛО = 25.0

болсын СалемдесуФункциясы = фн(АДАМ_АТЫ, САН) {
    болсын ТОЛЫҚ_СӨЙЛЕМ = СӨЙЛЕМ + ", " + АДАМ_АТЫ + "!"

    егер (САН > БАЗА_ЧИСЛО) {
        болсын ТУРЕЦКАЯ_ФРАЗА_ДОБАВЛЕНА = шын
        болсын ТОЛЫҚ_СӨЙЛЕМ = ТОЛЫҚ_СӨЙЛЕМ + " " + ТУРЕЦКОЕ_СЛОВО + " Türkiye'den!"
    }
    егер (САН < БАЗА_ЧИСЛО) {
        болсын ТУРЕЦКАЯ_ФРАЗА_ДОБАВЛЕНА = жалған
    }

    қайтару ТОЛЫҚ_СӨЙЛЕМ
}

болсын ПАЙДАЛАНУШЫ_АТЫ = "Әлия"
болсын ПАЙДАЛАНУШЫ_САНЫ = 27.5

~trk
eğer (ТУРЕЦКАЯ_ФРАЗА_ДОБАВЛЕНА == doğru) {
    olsun SON_UZUNLUK = uzunluk(СалемдесуФункциясы(ПАЙДАЛАНУШЫ_АТЫ, ПАЙДАЛАНУШЫ_САНЫ))
    döndür SON_UZUNLUK
}
eğer (ТУРЕЦКАЯ_ФРАЗА_ДОБАВЛЕНА == yanlış) {
    döndür "Türkçe ifade eklenmedi."
}